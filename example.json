{
  "manifest_version": 2,
  "name": "Speedify",
  "version": "1.0.0",
  "description": "Speed up videos on the page.",
  "short_name": "Speedify",
  "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["background.css"],
      "js": ["background.js"]
    }
  ],
  "browser_action": {
    "default_title": "Speed that video uuuuuup!",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png"
    }
  }
}
